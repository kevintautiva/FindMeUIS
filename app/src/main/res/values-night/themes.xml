<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.App1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00BBD3</item>
        <item name="colorPrimaryVariant">#2E2F3F</item>
        <item name="colorOnPrimary">#20212d</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">#20212d</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#2E2F3F</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.App1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.App1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.App1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>